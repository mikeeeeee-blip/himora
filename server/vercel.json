{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/ecommerce/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/product/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "/index.js"
    },
    {
      "src": "/",
      "dest": "/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "index.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}

